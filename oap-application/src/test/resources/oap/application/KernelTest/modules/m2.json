{
  "name": "m2",
  "dependsOn": [
    "m1"
  ],
  "services": {
    "ServiceTwo": {
      "implementation": "oap.application.ServiceTwo",
      "parameters": {
        "j": 1,
        "one": "@service:ServiceOne"
      },
      "listen": {
        "someListener": "@service:ServiceOne"
      },
      "supervision": {
        "supervise": true
      },
      "dependsOn": [
        "ServiceOne"
      ]
    },
    "ServiceScheduled": {
      "implementation": "oap.application.ServiceScheduled",
      "supervision": {
        "schedule": true,
        "delay": "1s"
      }
    },
    "ServiceDepsList": {
      "implementation": "oap.application.ServiceDepsList",
      "parameters": {
        "deps": [
          "@service:ServiceOne",
          "@service:ServiceTwo"
        ]
      },
      "dependsOn": [
        "ServiceOne",
        "ServiceTwo"
      ]
    }
  }
}
